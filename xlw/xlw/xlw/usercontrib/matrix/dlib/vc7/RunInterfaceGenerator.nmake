

MAKEFILE=RunInterfaceGenerator.nmake
BUILDFLAG=$(ENVIRON)\buildflag
IFACE_EXE=$(XLW)\xlw\build\$(VC)\$(ENVIRON)\InterfaceGenerator.exe
IFACE_INPUT=..\common_source\Test.h
IFACE_OUTPUT=.\AutoGeneratedSource\xlwTest.cpp

INPUTS = $(MAKEFILE) $(IFACE_EXE) $(IFACE_INPUT)

ALL : $(BUILDFLAG)

$(BUILDFLAG) : $(INPUTS)
    "$(IFACE_EXE)" $(IFACE_INPUT) $(IFACE_OUTPUT)
	ECHO flagged > $@

CLEAN :
	-@ if EXIST $(BUILDFLAG) del $(BUILDFLAG)

REBUILD : CLEAN ALL
