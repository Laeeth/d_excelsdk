<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Template" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin\Debug\$(PROJECT_NAME).xll" prefix_auto="0" extension_auto="0" />
				<Option object_output="obj\Debug\" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-D_DEBUG" />
				</Compiler>
				<Linker>
					<Add library='&quot;$(XLW)\xlw\lib\libxlw-gcc-s-gd-5_0_2f0.a&quot;' />
				</Linker>
				<ExtraCommands>
					<Add before="make -f RunInterfaceGenerator.mak" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Target title="Release">
				<Option output="bin\Release\$(PROJECT_NAME).xll" prefix_auto="0" extension_auto="0" />
				<Option object_output="obj\Release\" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-Wall" />
					<Add option="-DNDEBUG" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library='&quot;$(XLW)\xlw\lib\libxlw-gcc-s-5_0_2f0.a&quot;' />
				</Linker>
				<ExtraCommands>
					<Add before="make -f RunInterfaceGenerator.mak BUILD=Release" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add directory="$(XLW)\xlw\include" />
			<Add directory=".\source" />
		</Compiler>
		<Linker>
			<Add option="-static" />
			<Add option="-static-libgcc" />
			<Add library="user32" />
			<Add library='&quot;$(XLW)\xlw\include\xlw\xlw.def&quot;' />
			<Add directory="..\..\..\lib" />
		</Linker>
		<Unit filename="source\cppinterface.h" />
		<Unit filename="source\source.cpp" />
		<Unit filename="source\xlwWrapper.cpp" />
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
